{
  "name": "qoolquotes-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npx tsc && vite build && npm run generate-sitemap",
    "generate-sitemap": "node ./src/global/utilities/generate-sitemap.js",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run vitest && npm run build"
  },
  "dependencies": {
    "@mantine/core": "^7.5.3",
    "@mantine/dates": "^7.12.2",
    "@mantine/form": "^7.5.3",
    "@mantine/hooks": "^7.5.3",
    "@mantine/modals": "^7.5.3",
    "@mantine/notifications": "^7.5.3",
    "@mantine/vanilla-extract": "^7.5.3",
    "@mui/material": "^6.4.3",
    "@reduxjs/toolkit": "^2.2.1",
    "@tabler/icons-react": "^2.47.0",
    "@tanstack/react-query": "^5.22.2",
    "@vanilla-extract/css": "^1.14.1",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "file-saver": "^2.0.5",
    "fs": "^0.0.1-security",
    "html-to-image": "^1.11.13",
    "password-validator": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "react-share": "^5.2.0",
    "reflect-metadata": "^0.2.2",
    "sitemap": "^8.0.0",
    "validator": "^13.12.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-redux": "^7.1.33",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vanilla-extract/vite-plugin": "^4.0.4",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-pwa": "^0.19.2",
    "vitest": "^1.3.1"
  }
}
